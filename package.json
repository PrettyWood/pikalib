{
  "name": "pikalib",
  "version": "0.0.0",
  "description": "A basic library with Vue2 SFC + TS",
  "main": "./dist/pikalib.cjs.js",
  "module": "./dist/pikalib.esm.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "check": "pnpm run check:vue && pnpm run check:ts",
    "check:vue": "vuedx-typecheck --no-pretty --vue src",
    "check:ts": "tsc --noEmit",
    "dev": "vite",
    "lint": "eslint --ext .js,.ts,.vue,.json .",
    "lint-fix": "eslint --fix --ext .js,.ts,.vue,.json .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PrettyWood/pikalib"
  },
  "author": "PrettyWood <em.jolibois@gmail.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx?": [
      "pnpm run check:ts"
    ],
    "*.vue": [
      "pnpm run check:vue"
    ]
  },
  "engines": {
    "node": ">= 14.15",
    "pnpm": ">= 5.14"
  },
  "dependencies": {
    "@vue/composition-api": "^1.0.0-beta.24",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^12.19.12",
    "@vue/babel-preset-app": "^4.5.10",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/composition-api": "^1.0.0-beta.24",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@vuedx/typecheck": "^0.4.1",
    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "^2.0.2",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^10.5.3",
    "rollup": "^2.36.1",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-typescript2": "^0.25.2",
    "rollup-plugin-vue": "^5.1.4",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "vite": "^2.0.0-beta.9",
    "vite-plugin-vue2": "^1.0.0",
    "vue-jest": "^4.0.0-rc.0",
    "vue-template-compiler": "^2.6.10"
  }
}
